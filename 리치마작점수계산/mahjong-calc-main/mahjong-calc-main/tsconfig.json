{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["dom"],
    "jsx": "react-jsx",
    "sourceMap": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noPropertyAccessFromIndexSignature": false
  },
  "include": ["./src"]
}
